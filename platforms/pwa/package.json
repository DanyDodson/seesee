{
  "name": "seesee-platforms-pwa",
  "version": "1.0.0",
  "author": "Dany Dodson",
  "description": "seesee.space progressive web app",
  "license": "MIT",
  "scripts": {
    "build": "pawjs --no-cache --env-config-path=./.prod.env --env=production build",
    "build:demo": "yarn clean && pawjs --no-cache --env-config-path=./.demo.env --env=production build",
    "start": "pawjs --no-cache --env=development start",
    "clean": "rm -rf ./dist && rm -rf ./node_modules && yarn install",
    "test": "pawjs --no-cache test"
  },
  "dependencies": {
    "@pawjs/image-optimizer": "^2.2.6",
    "@pawjs/pawjs": "^3.0.2-beta.6",
    "@pawjs/sass": "^2.2.5",
    "@pawjs/srcset": "^2.1.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "bulma": "^0.8.2",
    "cloudinary": "^1.21.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "prop-types": "^15.7.2",
    "universal-fetch": "^1.0.0",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "devDependencies": {}
}