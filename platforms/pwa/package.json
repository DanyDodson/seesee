{
  "name": "platforms-pwa",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Dany Dodson",
  "description": "platforms-pwa",
  "license": "MIT",
  "scripts": {
    "clean": "sudo rm -rf build",
    "build": "yarn clean && NODE_ENV=production parcel build src/index.html -d build/output --cache-dir build/cache",
    "watch": "yarn clean && parcel --port=7001 src/index.html -d build/output --cache-dir build/cache"
  },
  "dependencies": {
    "@types/react": "^16.8.0",
    "apollo-cache-inmemory": "~1.6.5",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "~1.5.16",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "~2.10.1",
    "react": "^16.8.0",
    "react-apollo": "~3.1.3",
    "react-dom": ">= 16.3.0",
    "react-hook-form": "~3.29.4",
    "react-redux": "~7.1.3",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "styled-components": "~4.4.1",
    "yup": "~0.28.0"
  },
  "devDependencies": {
    "parcel-bundler": "~1.12.4"
  },
  "alias": {
    "#root": "./src"
  }
}