    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "dayjs": "^1.9.6",
    "is-mobile": "^2.2.2",
    "jwt-decode": "^3.1.2",
    "notistack": "^1.0.2",
    "overmind": "^26.0.0",
    "overmind-react": "^27.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "express": "^4.17.1",
